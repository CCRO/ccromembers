<div class="span4 offset4">
  <center><%= image_tag "ccro_long.png" %></center><br />
  <%= form_tag sessions_path, :class => "well" do %>
    <div class="field">
      <%= label_tag :email %><br />
      <%= text_field_tag :email, nil, :placeholder => "your@email.com" %>
    </div>
    <div class="field">
      <%= label_tag :password %><br />
      <%= password_field_tag :password %>
    </div>

    <div class="actions"><%= submit_tag "Log In", :class => "btn" %> or <%= link_to 'register', new_person_path %></div>
  <% end%>
</div>
